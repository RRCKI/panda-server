  `TOTRBYTES` INT(10) DEFAULT NULL,
  `TOTWBYTES` INT(10) DEFAULT NULL,
  `RATERCHAR` INT(10) DEFAULT NULL,
  `RATEWCHAR` INT(10) DEFAULT NULL,
