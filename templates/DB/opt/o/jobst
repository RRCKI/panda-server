  `WORKQUEUE_ID` int(11) DEFAULT NULL,
  `JEDITASKID` bigint(20) DEFAULT NULL,
  `JOBSUBSTATUS` varchar(80) DEFAULT NULL,
  `ACTUALCORECOUNT` int(11) DEFAULT NULL,

  `REQID` int(11) DEFAULT NULL,
