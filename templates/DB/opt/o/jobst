  CONSTRAINT `JOBSARCHIVED4_WORKQUEUE_ID_FK` FOREIGN KEY (`WORKQUEUE_ID`) REFERENCES `jedi_work_queue` (`QUEUE_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

